      "package": "com.rafael86.WomenPeriodApp",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "f76653c9-89c5-4e17-b8d5-e996af740da9"
      }
    }
  }
}