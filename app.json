{
  "expo": {
    "name": "WomenPeriodApp",
    "slug": "WomenPeriodApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logowomen.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logonotification.png",
          "color": "#fecdd3"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logowomen.png",
        "backgroundColor": "#fecdd3"
